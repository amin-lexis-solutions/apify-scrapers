    "@types/franc": "^5.0.3",
    "@types/langdetect": "^0.2.2",
    "@types/lru-cache": "^7.10.10",
    "@types/swagger-ui-express": "^4.1.6",
    "ngrok": "^5.0.0-beta.2",
    "tldts": "^6.1.21",
    "tsc-alias": "1.8.8",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  }
}
