    "@prisma/client": "^5.6.0",
    "@sentry/node": "^7.112.2",
    "@sentry/profiling-node": "^7.112.2",
    "@types/node-fetch": "2.x",
    "@types/progress": "^2.0.7",
    "apify": "3.1.14",
    "apify-client": "^2.9.5",
    "apitally": "^0.8.1",
    "async-mutex": "^0.5.0",
    "chrono-node": "^2.7.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "class-validator-jsonschema": "^5.0.0",
    "cld": "^2.9.1",
    "compression": "^1.7.4",
    "dayjs": "^1.11.11",
    "dotenv": "^16.3.1",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "langdetect": "^0.2.1",
    "lru-cache": "^10.2.0",
    "moment": "^2.29.4",
    "node-fetch": "2.x",
    "pg": "^8.11.3",
    "prisma": "^5.6.0",
    "progress": "^2.0.3",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.4",
    "routing-controllers-openapi": "^4.0.0",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
