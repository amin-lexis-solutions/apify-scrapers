{
  "name": "api",
  "version": "1.0.0",
  "description": "This project involves creating a TypeScript-based API that integrates with Apify for automated web scraping, stores data in a PostgreSQL database, and allows client data retrieval through well-documented endpoints.",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/server.ts --watch",
    "start": "node ./dist/api/src/server.js",
    "build": "tsc && tsc-alias ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma:dev": "./node_modules/.bin/prisma migrate dev --schema prisma/schema.prisma",
    "prisma:generate": "./node_modules/.bin/prisma generate --schema prisma/schema.prisma",
    "prisma:migrate": "./node_modules/.bin/prisma migrate deploy --schema prisma/schema.prisma",
    "prisma:reset": "./node_modules/.bin/prisma migrate reset --schema prisma/schema.prisma",
    "prisma:reset:testing": "source .env.testing && export $(cut -d= -f1 .env.testing) && yarn prisma:reset",
    "prisma:seed": "ts-node prisma/seeds/populate_all_target_locales.ts",
    "prisma:seed:migrate-domains": "ts-node prisma/seeds/migrate_domains.ts",
    "prisma:seed:target-locales": "ts-node prisma/seeds/populate_all_target_locales.ts",
    "prisma:studio": "./node_modules/.bin/prisma studio --schema prisma/schema.prisma",
    "data-migration:01_add-locale-column-coupon": "ts-node prisma/migrations/20240524102226_add_locale_column_to_coupon/data-migration.ts",
    "data-migration:02_fix_coupon_locales": "ts-node -r tsconfig-paths/register  prisma/data-migrations/fix_coupon_locales.ts",
    "data-migration:03_apifyActorId_as_key": "./node_modules/.bin/ts-node -r tsconfig-paths/register prisma/migrations/20240615144927_add_apify_actor_id_coupon_source_domain_process_run/data-migration.ts",
    "data-migration:04_flag_fake_coupon_code": "ts-node -r tsconfig-paths/register prisma/data-migrations/flag_fake_coupon_code.ts",
    "data-migration:05_apifyActor_changeOwner": "./node_modules/.bin/ts-node -r tsconfig-paths/register prisma/data-migrations/migrate_actor_id.ts migrate",
    "data-migration:06_apifyActor_cleanup": "./node_modules/.bin/ts-node -r tsconfig-paths/register prisma/data-migrations/migrate_actor_id.ts cleanup",
    "scripts:generate-wiki": "ts-node scripts/wiki-locale-generator.ts",
    "scripts:run-cost": "ts-node -r tsconfig-paths/register  scripts/actors-run-costs.ts",
    "scripts:serp-cost": "ts-node -r tsconfig-paths/register  scripts/serp-run-cost.ts",
    "scripts:load-serp": "ts-node -r tsconfig-paths/register  scripts/load_serp_results.ts",
    "scripts:tests": "ts-node -r tsconfig-paths/register  scripts/automated-tests.ts",
    "scripts:sync-merchants-data": "ts-node -r tsconfig-paths/register scripts/sync-merchants-data.ts",
    "scripts:migrate-old-sources": "ts-node -r tsconfig-paths/register scripts/migrate-old-sources.ts",
    "scripts:match-merchants-and-target-pages": "ts-node -r tsconfig-paths/register scripts/match-merchants-and-target-pages.ts",
    "scripts:match-merchants-and-coupons": "ts-node -r tsconfig-paths/register scripts/match-merchants-and-coupons.ts",
    "scripts:match-sourceDomains-and-coupons": "ts-node -r tsconfig-paths/register scripts/match-sourceDomains-and-coupons.ts",
    "scripts:match-sourceDomains-locales-and-coupons": "ts-node -r tsconfig-paths/register scripts/match-sourceDomains-locales-and-coupons.ts",
    "scripts:verify-runnable-actors-are-shared-with-oberst": "ts-node -r tsconfig-paths/register scripts/verify-runnable-actors-are-shared-with-oberst.ts",
    "schedule:actors": "./node_modules/.bin/ts-node -r tsconfig-paths/register  ./src/crons/schedule-actors.ts",
    "schedule:find-serp": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./src/crons/schedule-find-serp.ts",
    "schedule:find-serp-for-custom-malaysian-domains": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./src/crons/schedule-find-serp-for-custom-malaysian-domains.ts",
    "schedule:find-serp-for-cuponation-domains": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./src/crons/schedule-find-serp-for-cuponation-domains.ts",
    "schedule:find-serp-for-custom-ch-domains": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./src/crons/schedule-find-serp-for-custom-domains.ts",
    "schedule:tests": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./src/crons/schedule-test.ts",
    "schedule:refresh-unfinished-processed-runs": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./src/crons/refresh-unfinished-processed-runs.ts",
    "schedule:periodic-coupons-cleanup": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./src/crons/schedule-coupons-cleanup.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lexis-solutions/oberst-scrapers.git"
  },
  "keywords": [
    "TypeScript",
    "API",
    "Apify",
    "web",
    "scraping",
    "data",
    "retrieval",
    "PostgreSQL",
    "automation"
  ],
